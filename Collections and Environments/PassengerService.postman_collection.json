					"listen": "test",
					"script": {
						"exec": [
							"var template = `",
							"<style type=\"text/css\">",
							"    .tftable {font-size:14px;color:#333333;width:100%;border-width: 1px;border-color: #87ceeb;border-collapse: collapse;}",
							"    .tftable th {font-size:18px;background-color:#87ceeb;border-width: 1px;padding: 8px;border-style: solid;border-color: #87ceeb;text-align:left;}",
							"    .tftable tr {background-color:#ffffff;}",
							"    .tftable td {font-size:14px;border-width: 1px;padding: 8px;border-style: solid;border-color: #87ceeb;}",
							"    .tftable tr:hover {background-color:#e0ffff;}",
							"</style>",
							"",
							"<table class=\"tftable\" border=\"1\">",
							"    <tr>",
							"        <th>Passenger ID</th>",
							"        <th>Name</th>",
							"        <th>Mobile</th>",
							"        <th>Gender</th>",
							"        <th>Aadhaar Number</th>",
							"        <th>Address</th>",
							"    </tr>",
							"    ",
							"    {{#each response}}",
							"        <tr>",
							"            <td>{{passengerId}}</td>",
							"            <td>{{passengerName}}</td>",
							"            <td>{{passengerMobile}}</td>",
							"            <td>{{passengerGender}}</td>",
							"            <td>{{passengerAadhaarNumber}}</td>",
							"            <td>{{passengerAddress}}</td>",
							"        </tr>",
							"    {{/each}}",
							"</table>",
							"`;",
							"",
							"function constructVisualizerPayload() {",
							"    return {response: pm.response.json()};",
							"}",
							"",
							"pm.visualizer.set(template, constructVisualizerPayload());",
							"",
							"pm.test(\"Response status code is 200\", function () {",
							"    pm.expect(pm.response.code).to.eql(200);",
							"});",
							"",
							"",
							"pm.test(\"Response time is less than 200ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(200);",
							"});",
							"",
							"",
							"pm.test(\"Response is an array with at least one passenger object\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    pm.expect(responseData[0]).to.be.an('object');",
							"});",
							"",
							"",
							"pm.test(\"Validate the passenger object schema for required fields\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    ",
							"    responseData.forEach(passenger => {",
							"        pm.expect(passenger).to.have.all.keys('passengerId', 'passengerName', 'passengerMobile', 'passengerGender', 'passengerAadhaarNumber', 'passengerAddress');",
							"        ",
							"        pm.expect(passenger.passengerId).to.be.a('number');",
							"        pm.expect(passenger.passengerName).to.be.a('string').and.to.have.lengthOf.at.least(1, \"passengerName should not be empty\");",
							"        pm.expect(passenger.passengerMobile).to.be.a('string').and.to.have.lengthOf.at.least(1, \"passengerMobile should not be empty\");",
							"        pm.expect(passenger.passengerGender).to.be.a('string').and.to.have.lengthOf.at.least(1, \"passengerGender should not be empty\");",
							"        pm.expect(passenger.passengerAadhaarNumber).to.be.a('string').and.to.have.lengthOf.at.least(1, \"passengerAadhaarNumber should not be empty\");",
							"        pm.expect(passenger.passengerAddress).to.be.a('string').and.to.have.lengthOf.at.least(1, \"passengerAddress should not be empty\");",
							"    });",
							"});",
							"",
							"",
							"pm.test(\"PassengerMobile is in a valid mobile number format\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    responseData.forEach(function(passenger) {",
							"        pm.expect(passenger.passengerMobile).to.match(/^\\+?[1-9]\\d{1,14}$/, \"PassengerMobile format is invalid\");",
							"    });",
							"});",
